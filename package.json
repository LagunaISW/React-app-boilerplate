{
  "name": "react-app",
  "version": "0.0.1",
  "description": "Initial configuration for ReactJS projects",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:prod && npm run start:prod",
    "dev": "docker-compose up --build",
    "build:dev": "NODE_ENV=development webpack --progress --hide-modules --config webpack.dev.js",
    "build:prod": "NODE_ENV=production webpack -p",
    "start:dev": "NODE_ENV=development webpack-dev-server --progress --color --config webpack.dev.js",
    "start:prod": "NODE_ENV=production node index.js",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:LagunaISW/React-app-boilerplate.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Jhonatan Laguna",
  "license": "MIT",
  "homepage": "https://github.com/LagunaISW/React-app-boilerplate/blob/master/README.md",
  "nodemonConfig": {
    "ignore": [
      "src",
      "test"
    ]
  },
  "semistandard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "cookie-parser": "^1.4.3",
    "jsonwebtoken": "^7.2.1",
    "moment": "^2.17.1",
    "strength": "^0.1.4",
    "validator": "^7.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.0.2",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-template": "^6.0.2",
    "mocha": "^3.2.0",
    "moment-timezone": "^0.5.13",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-avatar": "^2.5.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-hot-loader": "~3.1.1",
    "react-native-rest-client": "^0.1.1",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scroll": "^1.5.5",
    "react-select": "^1.0.0-rc.4",
    "react-syntax-highlighter": "^6.0.4",
    "react-tooltip": "^3.3.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^0.16.0",
    "semistandard": "^12.0.1",
